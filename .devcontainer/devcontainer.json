{
	"name": "F# (.NET Core 5.0)",
	"dockerFile": "Dockerfile",
	"settings": {
			"terminal.integrated.shell.linux": "/bin/bash",
			"FSharp.useSdkScripts":true,
			"FSharp.workspacePath": "FsAutoComplete.sln"
		},
	"onCreateCommand": "dotnet tool restore && dotnet restore",
	"extensions": [
			"Ionide.Ionide-fsharp",
			"Ionide.Ionide-Paket",
			"ms-vscode.csharp"
		]
}
